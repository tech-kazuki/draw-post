<div class="devise">
  <h2>Log in</h2>

  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field">
      <%= f.label :email, class: 'devise_str' %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email", class: 'devise_str' %>
    </div>

    <div class="field">
      <%= f.label :password, class: 'devise_str' %><br />
      <%= f.password_field :password, autocomplete: "current-password", class: 'devise_str' %>
    </div>

    <% if devise_mapping.rememberable? %>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me, class: 'devise_str' %>
      </div>
    <% end %>

    <div class="actions">
      <%= f.submit "Log in" %>
    </div>
      <%= render "devise/shared/links" %>
  <% end %>
</div>